<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="cvPane" prefHeight="588.0" prefWidth="1136.0" styleClass="section_views" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="logic.presentation.control.CVInfoGraphic">
   <children>
      <Button fx:id="uploadCV" layoutX="273.0" layoutY="56.0" mnemonicParsing="false" onAction="#selectCV" text="Upload">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="1081.0" layoutY="25.0" mnemonicParsing="false" onAction="#closeCVSection" prefHeight="25.0" prefWidth="25.0" styleClass="close_btn">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <TextField layoutX="47.0" layoutY="56.0" prefHeight="25.0" prefWidth="226.0" />
      <Hyperlink fx:id="cvLink" layoutX="48.0" layoutY="57.0" onAction="#showCV" prefHeight="23.0" prefWidth="226.0" text="There is no CV yet" />
   </children>
</AnchorPane>
